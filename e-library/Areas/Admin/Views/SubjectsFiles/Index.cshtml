@model IEnumerable<e_library.Models.SubjectFiles>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SubjectFileName)
            </th>
            
            <th>
                @Html.DisplayNameFor(model => model.SubjectDescription)
            </th>
          
            <th>
                @Html.DisplayNameFor(model => model.Subjects)
            </th>
             <th>
                @Html.DisplayNameFor(model => model.SubjectFileURL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LookupMediaTypeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubjectFileURLOthers)
            </th>
            
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SubjectFileName)
            </td>
           @* <td>
                    <a href="~/Images/@item.SubjectFileURL" target="_blank">    <img src="~/Images/@item.SubjectFileURL" style="width:60px; /></a>
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.SubjectDescription)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SubjectId)
                </td>
                <td>

                    <a href="~/Images/@item.SubjectFileURL" target="_blank">    <img src="~/Images/@item.SubjectFileURL" /></a>
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.LookupMediaTypeId)
                </td>
           
                <td>
                    <a href="@item.SubjectFileURLOthers">@item.SubjectFileURLOthers</a>
                </td>
           
               


            <td>
                <a asp-action="Edit" asp-route-id="@item.SubjectFileId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SubjectFileId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SubjectFileId">Delete</a>
            </td>

                
        </tr>
}
    </tbody>
</table>
